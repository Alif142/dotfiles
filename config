order += "cpu_usage"
order += "memory"
order += "disk /"
#order += "load"
order += "tztime local"

cpu_usage {
    format = "CPU: %usage"
}

memory {
    format = "Mem: %used / %available"
}

disk "/" {
    format = "Disk: %avail"
}

load {
    format = "Load: %1min, %5min, %15min"
}

tztime local {
    format = "Time: %Y-%m-%d %H:%M:%S"
}
